pipeline{
    agent any
    stages{
        stage('stage 1 - Hacer docker version'){
            steps{
                echo 'WELCOME TO CAFETO FILE SERVICE PROJECT'
                echo 'Hacer docker build'
                sh 'docker --version'
            }
        }
        stage('stage 2 - Hacer jenkins version'){
            steps{
                echo 'jenkins --version'
            }
        }
    }
}

